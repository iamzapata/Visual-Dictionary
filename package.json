{
  "name": "VisualDictionary",
  "version": "1.0.0",
  "description": "Dictionary that displays images and definitions of search terms.",
  "repository": "url",
  "author": "Andres Zapata",
  "license": "MIT",
  "scripts": {
    "test": "jest test",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "dev": "NODE_ENV=development webpack-dev-server --mode=development --open --hot",
    "server": "node server/index.js",
    "build": "NODE_ENV=production webpack --mode=production",
    "prettier": "prettier --write 'src/**/*'",
    "prettier:watch": "onchange 'src/**/*' -- npx prettier --write {{changed}}",
    "start": "node index.js",
    "deploy:local": "TARGET=deploy yarn build && now && now alias",
    "deploy": "TARGET=deploy now && now alias",
    "webpack:analyse": "BUNDLE_ANALYZER=true yarn build"
  },
  "dependencies": {
    "autoprefixer": "9.1.3",
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "hapi": "^17.5.3",
    "inert": "^5.1.0",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-slick": "^0.23.1",
    "react-spinners": "0.4.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "shortid": "^2.2.12"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-class-properties": "7.0.0-beta.3",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "bulma": "^0.7.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "fetch-mock": "^6.4.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "jest-dom": "^1.12.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "4.9.3",
    "onchange": "^4.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.14.2",
    "react-testing-library": "^5.0.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^7.1.0",
    "slick-carousel": "^1.8.1",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.10.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-dotenv-plugin": "^2.0.2"
  },
  "sideEffects": [
    "*.css"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/src/jest/globals.js"
    ],
    "testURL": "http://localhost",
    "automock": false,
    "moduleNameMapper": {
      "^Components(.*)$": "<rootDir>/src/Components$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "API_URL": "",
      "APP_ID": "",
      "APP_KEY": "",
      "GOOGLE_SEARCH_KEY": "",
      "GOOGLE_SEARCH_ENGINE": ""
    }
  }
}
