{
  "name": "VisualDictionary",
  "version": "1.0.0",
  "description": "Dictionary that displays images and definitions of search terms. Also allows search by image\"",
  "repository": "url",
  "author": "Andres Zapata",
  "license": "MIT",
  "scripts": {
    "test": "jest test",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "start": "NODE_ENV=development webpack-dev-server --mode development --open --hot",
    "build": "NODE_ENV=production webpack",
    "prettier": "prettier --write 'src/**/*'",
    "prettier:watch": "onchange 'src/**/*' -- npx prettier --write {{changed}}"
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "webpack-dotenv-plugin": "^2.0.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "fetch-mock": "^6.4.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.0.1",
    "onchange": "^4.0.0",
    "prettier": "1.13.5",
    "react-testing-library": "^3.1.3",
    "redux-mock-store": "^1.5.1",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "automock": false,
    "moduleNameMapper": {
      "^Components(.*)$": "<rootDir>/src/Components$1",
      "^utils(.*)$": "<rootDir>/src/utils$1"
    },
    "globals": {
      "API_URL": "",
      "APP_ID": "",
      "APP_KEY": ""
    }
  }
}
